{
  "name": "@saltcorn/json",
  "version": "0.4.2",
  "description": "JSON field plugin",
  "main": "json.js",
  "dependencies": {
    "@saltcorn/markup": "^0.4.0"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
